{
    "extends": ["react-app", "eslint:recommended", "prettier"],
    "plugins": [],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "import/no-anonymous-default-export": "off",
        "no-unused-vars": "off"
    }
}
